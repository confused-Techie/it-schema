<% if (Array.isArray(data) && data.length > 0) { %>
  <ul>
    <% for (const item of data) { %>
      <li>
        <a href="/<%=item["rdfs:label"]%>">
          <%=item["rdfs:label"]%>
        </a>
        <%-include("jsonld_tree_node.ejs", { data: item["@children"] })%>
      </li>
    <% } %>
  </ul>
<% } %>
